namespace bond.comm.tcp;

enum PayloadType
{
    Request = 1;
    Response = 2;
    Event = 3;
}

struct TcpHeaders
{
    0: uint32 request_id;
    1: required PayloadType payload_type;
    2: string method_name;
    3: int32 error_code;
}
